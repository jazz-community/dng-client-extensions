	</ju:catalog-entry>
	<ju:catalog-entry>
		<dc:description>This extension changes the link type of selected requirements.</dc:description>
		<dc:title>Change Link Type</dc:title>
		<ju:gadget rdf:resource="./change-link/change-link.xml" />
		<ju:icon rdf:resource="./change-link/change-link_icon.png"/>
		<ju:preview rdf:resource="./change-link/change-link_pre.png"/>
		<ju:thumbnail rdf:resource="./change-link/change-link_thumb.png"/>
		<ju:category>Requirements Extensions</ju:category> 
