	xmlns:ju="http://jazz.net/ns/ui#" 
	xmlns:dc="http://purl.org/dc/terms/">
	<ju:catalog-entry>
		<dc:description>This extension searches for [%attribute=value] in primary text and transfers the value to the corresponding attribute.</dc:description>
